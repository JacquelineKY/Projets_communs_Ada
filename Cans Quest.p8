--echange

function alu_coin()
  if p.x==91 and p.y==22 then
  while p.cans > 0 do
        newcoins = p.cans * 5
        p.cans = 0
        p.coins = p.coins + newcoins
	 end
	end
end

function depot_bank()
 if p.x==10 and p.y==25 then
 while p.coins > 0 do
 						p.bank_account=p.bank_account+p.coins
 						p.coins=0
  end		
 end
end
