function init_msg()
		messages={}
end

function create_msg(name,...)
	msg_title=name
	messages={...}
end

function update_msg()
		if (btn(❎)) then
			deli(messages,1)
	 end
end

function draw_msg()
  if messages[1] then
  		local x=45
  		local y=45
  		--titre
  		rectfill(x-2,y-2,
  		x+#msg_title*4,y+6,0)
  		rect(x-2,y-2,
  		x+#msg_title*4,y+7,1)
  		print(msg_title,x,y,7)
  		--message
  		rectfill(x-2,y+7,100,y+35,0)
  		rect(x-2,y+7,100,y+35,1)
  		print(messages[1],x,y+10,7)
  end
end
