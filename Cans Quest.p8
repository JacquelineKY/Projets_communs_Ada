--ramassage canettes

function pick_up_red_canette(x,y)
  next_tile(x,y)
  if fget(11,1) then
  p.cans += 1
  sfx(1)
 end
end

function pick_up_green_canette(x,y)
  next_tile(x,y)
  if fget(13,2) then
  p.cans += 2
  sfx(1)
 end
end

function pick_up_blue_canette(x,y)
    next_tile(x,y)
    if fget(9,3) then
    p.cans += 5
    sfx(1)
    else
   end
end

function pick_up_yellow_canette(x,y)
    next_tile(x,y)
    if fget(25,4) then
    p.cans += 10
    sfx(1)
    else
  end
end

function interact_canette(x,y)
  if check_flag (1,x,y) then
      pick_up_red_canette(x,y)
  else if check_flag(2,x,y) then
      pick_up_green_canette(x,y)
  else if check_flag(3,x,y) then
      pick_up_blue_canette(x,y) 
  else if check_flag(4,x,y) then
      pick_up_yellow_canette(x,y)
		 end
	 end
 end
 end
end


