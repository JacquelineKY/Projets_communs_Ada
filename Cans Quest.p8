--player

function create_player()
	p={
	x=0,y=10,
	ox=0,oy=0,
	start_ox=0,start_oy=0,
	anim_t=0,
	sprite=1,
 cans=0,
 coins=0,
	bank_account=0
	}
end


function player_movement()
   newx=p.x
   newy=p.y
  if p.anim_t==0 then
  newox,newoy=0,0
	if (btn(➡️)) then 
		newx+=1
		newox=-8
	 p.flip=false
	elseif (btn(⬅️)) then
 	newx-=1 
	 newox=8
		p.flip=true
	elseif (btn(⬇️)) then
	 newy+=1 
		newoy=-8
	 
	elseif (btn(⬆️)) then
		newy-=1 
		newoy=8
		end
		end
	
	interact(newx,newy)
	interact_canette(newx,newy)
	
	if not check_flag(0,newx,newy)
		and (p.x!=newx or p.y!=newy) then
		p.x=mid(0,newx,127)
		p.y=mid(0,newy,31)
		p.start_ox=newox
		p.start_oy=newoy
		p.anim_t=1
 end
 --animation
 p.anim_t=max(p.anim_t-0.125,0)
 p.ox=p.start_ox*p.anim_t
 p.oy=p.start_oy*p.anim_t
 
 if p.anim_t>=0.5 then
 	p.sprite=2
 else 
	 p.sprite=1
	 end
end

function interact(x,y)
	if check_flag(1,x,y) then
	pick_up_canette(x,y)
	end
	if x==2 and y==8 then
			create_msg("panneau",
			"la banque \nest au sud")
			end
	if x==3 and y==8 then
			create_msg("panneau",
			"le magasin \nd'echange \nest a l'est")
			end
	if x==48 and y==24 and not talked_pnj then 
				create_msg("akemi",
			 "je te donne \nles canettes \nque mon mari \na bu hier soir")
			 p.cans += 7
			 talked_pnj=true
			else if x==48 and y==24 and talked_pnj then 
			 create_msg("akemi", "j'aimerais \nqu'il arrete \nde boire tous \nles soirs ...")
			end
		end	
	if x==67 and y==10 and not talked_pnj1 then
				create_msg("shizuka", "tu veux la \ncanette que \nje viens de \nboire ??")
				p.cans += 1
				talked_pnj1=true
			else if	x==67 and y==10 and talked_pnj1 then
				create_msg("shizuka", "merci \nbeaucoup !")
				end
			end
end

function draw_player()
	spr(p.sprite,p.x*8+p.ox,p.y*8+p.oy,1,1,p.flip)

end

function draw_ui()
    camera()
    palt(0,false)
    palt(5,true) -- fond alpha symbole hud
    spr(11,2,1)
    print("x"..p.cans,10,2,7)
    palt()
    spr(39,1,12)
    print("x"..p.coins,10,14,7)
    palt()
    spr(41,1,24)
    print("x"..p.bank_account.."/300",10,25,7)
end
