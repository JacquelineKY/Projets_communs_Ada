--start game

function init_game()
	game_over=false
	victoire=false
	create_player()
	newx=p.x
	newy=p.y
	--	create_enemy()
	init_msg()
	_update=update_game
	_draw=draw_game
end

function update_game()
	if (game_over) then
			init_endgame()
	elseif (victoire) then
			init_victory()
	elseif(not game_over and not victoire) then
			check_endgame()
			check_victory()
			if not messages[1] then
						player_movement()
			end
			update_camera()
			update_msg()
			alu_coin()
			depot_bank()
			--	jack_the_ripper()
end
end



function draw_game()
	cls()
	draw_map()
	draw_player()
	draw_ui()
	draw_msg()
--	draw_enemy()
end

